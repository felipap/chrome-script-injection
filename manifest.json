{
  "manifest_version": 2,
  "name": "Sub Script Tester",
  "options_page": "options.html",
  "version": "1.0.0",
  "icons": {
    "48": "assets/icon_48.png",
    "128": "assets/icon_128.png"
  },
  "browser_action": {
    "default_icon": "assets/icon_128.png",
    "default_title": "Inject Script",
    "default_popup": "dist/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "build/script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "storage",
    "unlimitedStorage",
    "webNavigation",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "build/background.js"
    ],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' https://storage.googleapis.com/human-static; object-src 'self'"
}